{
  "manifest_version": 2,

  "name": "mark-bookmarked-links",
  "description": "mark-bookmarked-links",
  "version": "1.0",

  "permissions": [
    "bookmarks"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["content.css"],
      "js": ["content.js"]
    }
  ]
}
