{
  "manifest_version": 2,

  "name": "mark-bookmarked-links",
  "description": "mark-bookmarked-links",
  "version": "1.0",

  "permissions": [
    "bookmarks",
    "storage"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
